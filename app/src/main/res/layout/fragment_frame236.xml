<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.FragmentFrame236">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:alpha="0.5"
            android:layout_gravity="top"
            android:background="@drawable/shadow_top" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="30dp"
            android:layout_marginVertical="20dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="화면 설정"
                android:textSize="20dp"
                android:paddingHorizontal="13dp"
                android:paddingVertical="5dp"
                android:background="#CAC3F1"
                android:fontFamily="@font/inter"/>

            <TextView
                android:id="@+id/button31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="저장1"
                android:textSize="20dp"
                android:layout_marginHorizontal="9dp"
                android:paddingHorizontal="10dp"
                android:paddingVertical="5dp"
                android:fontFamily="@font/inter"
                android:background="#C7C7C7"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="저장2"
                android:textSize="20dp"
                android:paddingHorizontal="13dp"
                android:paddingVertical="5dp"
                android:fontFamily="@font/inter"
                android:background="#C7C7C7"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_margin="30dp"
            android:paddingBottom="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/header">

            <!-- Row 1 -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginBottom="8dp">


                <com.google.android.material.card.MaterialCardView
                    android:layout_width="238dp"
                    android:layout_height="103dp"
                    android:id="@+id/cardf_1"
                    android:layout_gravity="center"

                    android:elevation="7dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="7dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="#000000">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|center_vertical"
                        android:text="고압"
                        android:background="#F2F0FF"
                        android:textColor="#0B0759"
                        android:fontFamily="@font/inter"
                        android:textFontWeight="800"
                        app:fontWeight="700"
                        tools:ignore="HardcodedText"




                        android:textSize="20dp" />
                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="238dp"
                    android:layout_height="103dp"
                    android:id="@+id/cardf_2"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="18dp"
                    android:elevation="7dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="7dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="#000000">


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="2개"
                        android:textColor="#0B0759"
                        android:fontFamily="@font/inter"
                        android:textFontWeight="800"
                        app:fontWeight="700"
                        tools:ignore="HardcodedText"




                        android:textSize="20dp" />
                </com.google.android.material.card.MaterialCardView>


                <com.google.android.material.card.MaterialCardView
                    android:layout_width="238dp"
                    android:layout_height="103dp"
                    android:id="@+id/cardf_3"
                    android:layout_gravity="center"
                    android:elevation="7dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="7dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="#000000">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="3개"
                        android:gravity="center"
                        android:textColor="#0B0759"
                        android:fontFamily="@font/inter"
                        android:textFontWeight="800"
                        app:fontWeight="700"
                        tools:ignore="HardcodedText"




                        android:textSize="20dp" />
                </com.google.android.material.card.MaterialCardView>


            </LinearLayout>

            <!-- Row 2 -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:layout_marginBottom="8dp">

                <com.google.android.material.card.MaterialCardView
                    android:layout_width="238dp"
                    android:layout_height="103dp"
                    android:id="@+id/cardf_4"
                    android:layout_gravity="center"
                    android:elevation="7dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="7dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="#000000">



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:fontFamily="@font/inter"
                        android:text="4개"
                        android:textColor="#0B0759"
                        android:textFontWeight="800"
                        android:textSize="20dp"
                        app:fontWeight="700"


                        tools:ignore="HardcodedText" />
                </com.google.android.material.card.MaterialCardView>


                <com.google.android.material.card.MaterialCardView
                    android:layout_width="238dp"
                    android:layout_height="103dp"
                    android:id="@+id/cardf_5"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="18dp"
                    android:elevation="7dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="7dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="#000000">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="5개"
                        android:gravity="center"
                        android:textColor="#0B0759"
                        android:fontFamily="@font/inter"
                        android:textFontWeight="800"
                        app:fontWeight="700"
                        tools:ignore="HardcodedText"




                        android:textSize="20dp" />
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:layout_width="238dp"
                    android:layout_height="103dp"
                    android:id="@+id/cardf_6"
                    android:layout_gravity="center"
                    android:elevation="7dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="7dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="#000000">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="6개"
                        android:gravity="center"
                        android:textColor="#0B0759"
                        android:fontFamily="@font/inter"
                        android:textFontWeight="800"
                        app:fontWeight="700"
                        tools:ignore="HardcodedText"




                        android:textSize="20dp" />
                </com.google.android.material.card.MaterialCardView>


            </LinearLayout>
        </LinearLayout>



    </LinearLayout>
</FrameLayout>